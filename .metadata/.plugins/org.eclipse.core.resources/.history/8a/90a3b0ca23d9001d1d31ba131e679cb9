package dataAccess;

import model.Order;
import model.Product;

public class DaoFactory {

	public static Dao<Product> createProductDao() {

		return new dataAccess.sqlserver.ProductDao();
	}
	
	public static Dao<Order> createOrderDao(){
		
		return new dataAccess.sqlserver.OrderDao();
	}
	
	public enum Daos{
		Memory, 
		SqlServer
	}
}
